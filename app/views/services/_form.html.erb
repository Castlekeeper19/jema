<%= simple_form_for [@church, @service] do |f| %>
  <%= f.input :name, label: "Church Name" %>
  <%= f.input :type, label: "Service Type"%>
  <%= f.input :day, label: "Day of Week"%>
  <%= f.input :start, label: "Start Time"%>
  <%= f.input :end, label: "End Time"%>
  <%= f.input :language, label: "Service Language"%>
  <%= f.submit "Submit", class: "btn btn-gradient" %>
<% end %>
